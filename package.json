{
  "name": "react-nats",
  "version": "0.1.1",
  "description": "React hooks for NATS messaging and JetStream",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "nats",
    "jetstream",
    "hooks",
    "websocket",
    "messaging",
    "kv",
    "key-value"
  ],
  "author": "jeff@jeffplourde.com",
  "license": "MIT",
  "peerDependencies": {
    "@nats-io/jetstream": "^3.0.2-1",
    "@nats-io/kv": "^3.0.2-1",
    "@nats-io/nats-core": "^3.0.2-1",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@testcontainers/nats": "^11.11.0",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "happy-dom": "^20.4.0",
    "jsdom": "^27.4.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18",
    "ws": "^8.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffplourde/react-nats.git"
  }
}
